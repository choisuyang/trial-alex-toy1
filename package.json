    "react-dom": "^16.12.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-icon": "^2.3.3",
    "semantic-ui-react": "^0.88.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
